<?xml version='1.0' encoding='utf-8'?>
<widget id="com.aumcsci.flashcards" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:gap="http://phonegap.com/ns/1.0">
    <feature name="Camera">
        <param name="browser-package" value="Camera" />
    </feature>
    <feature name="Device">
        <param name="browser-package" value="Device" />
    </feature>
    <name>FlashCards</name>
    <description>
Mobile Flash Card app, for Mobile Computing class project
    </description>
    <author email="compsciclub@aum.edu">
        Manal Ismail-Abdalla
        Jason Hammond
    </author>
    <content src="index.html" />
    <icon src="icon.png" />
    <access origin="http://*.w3schools.com" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="android-minSdkVersion" value="14" />
</widget>
